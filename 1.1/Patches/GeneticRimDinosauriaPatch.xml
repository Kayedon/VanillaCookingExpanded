<?xml version="1.0" encoding="utf-8" ?>
<Patch>


	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>GeneticRim Dinosauria Patch</li>
		</mods>

		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>


				
				<!-- Dino Milk -->

				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs/ThingDef[defName = "GR_DinoMilk"]</xpath>
					<value>


						<modExtensions>
							<li Class="VanillaCookingExpanded.Milk_Extension">
								<mustCapacity>30</mustCapacity>
								<awfulQualityAgeDaysThreshold>3</awfulQualityAgeDaysThreshold>
								<poorQualityAgeDaysThreshold>7</poorQualityAgeDaysThreshold>
								<normalQualityAgeDaysThreshold>16</normalQualityAgeDaysThreshold>
								<goodQualityAgeDaysThreshold>30</goodQualityAgeDaysThreshold>
								<excellentQualityAgeDaysThreshold>50</excellentQualityAgeDaysThreshold>
								<masterworkQualityAgeDaysThreshold>70</masterworkQualityAgeDaysThreshold>
								<legendaryQualityAgeDaysThreshold>120</legendaryQualityAgeDaysThreshold>

								<cheeseToTurnInto>VCE_DinoCheese</cheeseToTurnInto>
								<amount>30</amount>

							</li>
						</modExtensions>


					</value>
				</li>

				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs</xpath>
					<value>


						<ThingDef ParentName="MealBase">
							<defName>VCE_DinoCheese</defName>
							<label>dino "cheese"</label>
							<description>A fake cheese, from a fake milk. Best not to think where this came from.</description>
							<graphicData>
								<texPath>Things/Items/Cheese/VCE_DinoCheese</texPath>
								<graphicClass>Graphic_StackCount</graphicClass>
							</graphicData>
							<tradeability>Sellable</tradeability>
							<stackLimit>75</stackLimit>
							<statBases>
								<MarketValue>18</MarketValue>
								<Nutrition>0.07</Nutrition>
							</statBases>
							<thingCategories>
								<li>VCE_Cheese</li>
							</thingCategories>
							<ingestible>
								<preferability>MealLavish</preferability>
								<ingestEffect>EatMeat</ingestEffect>
								<ingestSound>Meal_Eat</ingestSound>
								<maxNumToIngestAtOnce>14</maxNumToIngestAtOnce>	
							</ingestible>
							<comps>
								<li Class="CompProperties_Rottable">
									<daysToRotStart>120</daysToRotStart>
									<rotDestroys>true</rotDestroys>
								</li>
								<li>
									<compClass>CompQuality</compClass>
								</li>
								<li Class="VanillaCookingExpanded.CompProperties_StackByQuality" />
							</comps>
							
						</ThingDef>


					</value>
				</li>




				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs/VanillaCookingExpanded.AcceptedMilksDef[defName = "VCE_AcceptedMilks"]/milks</xpath>
					<value>
						<li>GR_DinoMilk</li>
						

					</value>
				</li>







			</operations>
		</match>
	</Operation>


</Patch>

