<?xml version="1.0" encoding="utf-8" ?>
<Patch>


	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>GeneticRim Cosmic Horrors Patch</li>
		</mods>

		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>


				<!-- Eldritch Milk -->

				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs/ThingDef[defName = "GR_EldritchMilk"]</xpath>
					<value>


						<modExtensions>
							<li Class="VanillaCookingExpanded.Milk_Extension">
								<mustCapacity>50</mustCapacity>
								<awfulQualityAgeDaysThreshold>5</awfulQualityAgeDaysThreshold>
								<poorQualityAgeDaysThreshold>7</poorQualityAgeDaysThreshold>
								<normalQualityAgeDaysThreshold>16</normalQualityAgeDaysThreshold>
								<goodQualityAgeDaysThreshold>30</goodQualityAgeDaysThreshold>
								<excellentQualityAgeDaysThreshold>50</excellentQualityAgeDaysThreshold>
								<masterworkQualityAgeDaysThreshold>70</masterworkQualityAgeDaysThreshold>
								<legendaryQualityAgeDaysThreshold>140</legendaryQualityAgeDaysThreshold>

								<cheeseToTurnInto>VCE_EldritchCheese</cheeseToTurnInto>
								<amount>25</amount>

							</li>
						</modExtensions>


					</value>
				</li>

				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs</xpath>
					<value>


						<ThingDef ParentName="MealBase">
							<defName>VCE_EldritchCheese</defName>
							<label>eldritch cheese</label>
							<description>Ph'nglui mglw'nafh Cheese R'lyeh wgah'nagl fhtagn. The only brand of cheese that would please the Great Old Ones themselves.</description>
							<graphicData>
								<texPath>Things/Items/Cheese/VCE_EldritchCheese</texPath>
								<graphicClass>Graphic_StackCount</graphicClass>
							</graphicData>
							<tradeability>Sellable</tradeability>
							<stackLimit>75</stackLimit>
							<statBases>
								<MarketValue>20</MarketValue>
								<Nutrition>0.05</Nutrition>

							</statBases>
							<thingCategories>
								<li>VCE_Cheese</li>
							</thingCategories>
							<ingestible>
								<preferability>MealLavish</preferability>
								<ingestEffect>EatMeat</ingestEffect>
								<ingestSound>Meal_Eat</ingestSound>
								<joy>0.06</joy>
								<joyKind>Gluttonous</joyKind>
								<maxNumToIngestAtOnce>10</maxNumToIngestAtOnce>

								<outcomeDoers>
									<li Class="IngestionOutcomeDoer_GiveHediff">
										<hediffDef>ROM_SanityLoss</hediffDef>
										<severity>0.025</severity>
									</li>
								</outcomeDoers>	
							</ingestible>
							<comps>
								<li Class="CompProperties_Rottable">
									<daysToRotStart>80</daysToRotStart>
									<rotDestroys>true</rotDestroys>
								</li>
								<li>
									<compClass>CompQuality</compClass>
								</li>
								<li Class="VanillaCookingExpanded.CompProperties_StackByQuality" />
							</comps>
						</ThingDef>


					</value>
				</li>




				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs/VanillaCookingExpanded.AcceptedMilksDef[defName = "VCE_AcceptedMilks"]/milks</xpath>
					<value>
						<li>GR_EldritchMilk</li>

					</value>
				</li>







			</operations>
		</match>
	</Operation>


</Patch>

