<?xml version="1.0" encoding="utf-8" ?>
<Patch>


	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>

		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>



				
				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs/ThingDef[defName = "AA_BlueMilk"]</xpath>
					<value>


						<modExtensions>
							<li Class="VanillaCookingExpanded.Milk_Extension">
								<mustCapacity>30</mustCapacity>
								<awfulQualityAgeDaysThreshold>3</awfulQualityAgeDaysThreshold>
					<poorQualityAgeDaysThreshold>7</poorQualityAgeDaysThreshold>
					<normalQualityAgeDaysThreshold>16</normalQualityAgeDaysThreshold>
					<goodQualityAgeDaysThreshold>30</goodQualityAgeDaysThreshold>
					<excellentQualityAgeDaysThreshold>50</excellentQualityAgeDaysThreshold>
					<masterworkQualityAgeDaysThreshold>70</masterworkQualityAgeDaysThreshold>
					<legendaryQualityAgeDaysThreshold>130</legendaryQualityAgeDaysThreshold>

								<cheeseToTurnInto>VCE_BlueCheese</cheeseToTurnInto>
								<amount>30</amount>

							</li>
						</modExtensions>


					</value>
				</li>

				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs</xpath>
					<value>


						<ThingDef ParentName="MealBase">
							<defName>VCE_BlueCheese</defName>
							<label>blue cheese</label>
							<description>Blue cheese is cheese made with cultures of the mold Penicillium, giving it blue spots or veins. This carries a "distinct" smell.</description>
							<graphicData>
								<texPath>Things/Items/Cheese/VCE_BlueCheese</texPath>
								<graphicClass>Graphic_StackCount</graphicClass>
							</graphicData>
							<tradeability>Sellable</tradeability>
							<stackLimit>75</stackLimit>
							<statBases>
								<MarketValue>17</MarketValue>
								<Nutrition>0.07</Nutrition>
							</statBases>
							<thingCategories>
								<li>VCE_Cheese</li>
							</thingCategories>
							<ingestible>
								<preferability>MealLavish</preferability>
								<ingestEffect>EatMeat</ingestEffect>
								<ingestSound>Meal_Eat</ingestSound>
								<maxNumToIngestAtOnce>15</maxNumToIngestAtOnce>
							</ingestible>
							<comps>
								<li Class="CompProperties_Rottable">
									<daysToRotStart>60</daysToRotStart>
									<rotDestroys>true</rotDestroys>
								</li>
								<li>
									<compClass>CompQuality</compClass>
								</li>
								<li Class="VanillaCookingExpanded.CompProperties_StackByQuality" />
							</comps>
						</ThingDef>


					</value>
				</li>
				
				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs/ThingDef[defName = "AA_GowMilk"]</xpath>
					<value>


						<modExtensions>
							<li Class="VanillaCookingExpanded.Milk_Extension">
								<mustCapacity>30</mustCapacity>
								<awfulQualityAgeDaysThreshold>2</awfulQualityAgeDaysThreshold>
								<poorQualityAgeDaysThreshold>7</poorQualityAgeDaysThreshold>
								<normalQualityAgeDaysThreshold>16</normalQualityAgeDaysThreshold>
								<goodQualityAgeDaysThreshold>30</goodQualityAgeDaysThreshold>
								<excellentQualityAgeDaysThreshold>50</excellentQualityAgeDaysThreshold>
								<masterworkQualityAgeDaysThreshold>70</masterworkQualityAgeDaysThreshold>
								<legendaryQualityAgeDaysThreshold>120</legendaryQualityAgeDaysThreshold>

								<cheeseToTurnInto>VCE_GowCheese</cheeseToTurnInto>
								<amount>30</amount>

							</li>
						</modExtensions>


					</value>
				</li>

				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs</xpath>
					<value>


						<ThingDef ParentName="MealBase">
							<defName>VCE_GowCheese</defName>
							<label>gow cheese</label>
							<description>Gow cheese is greatly appreciated for its smooth, melting texture, and its very soft taste.</description>
							<graphicData>
								<texPath>Things/Items/Cheese/VCE_GowCheese</texPath>
								<graphicClass>Graphic_StackCount</graphicClass>
							</graphicData>
							<tradeability>Sellable</tradeability>
							<stackLimit>75</stackLimit>
							<statBases>
								<MarketValue>16</MarketValue>
								<Nutrition>0.06</Nutrition>
							</statBases>
							<thingCategories>
								<li>VCE_Cheese</li>
							</thingCategories>
							<ingestible>
								<preferability>MealLavish</preferability>
								<ingestEffect>EatMeat</ingestEffect>
								<ingestSound>Meal_Eat</ingestSound>
								<maxNumToIngestAtOnce>17</maxNumToIngestAtOnce>
							</ingestible>
							<comps>
								<li Class="CompProperties_Rottable">
									<daysToRotStart>60</daysToRotStart>
									<rotDestroys>true</rotDestroys>
								</li>
								<li>
									<compClass>CompQuality</compClass>
								</li>
								<li Class="VanillaCookingExpanded.CompProperties_StackByQuality" />
							</comps>
						</ThingDef>


					</value>
				</li>
				
				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs/VanillaCookingExpanded.AcceptedMilksDef[defName = "VCE_AcceptedMilks"]/milks</xpath>
					<value>
							<li>AA_BlueMilk</li>
							<li>AA_GowMilk</li>
					</value>
				</li>







			</operations>
		</match>
	</Operation>


</Patch>

