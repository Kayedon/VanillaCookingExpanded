<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 <WorkGiverDef>
    <defName>VCE_InsertSoup</defName>
    <label>bringing soup to electric pot</label>
    <giverClass>VanillaCookingExpanded.WorkGiver_InsertSoup</giverClass>
    <workType>Hauling</workType>
    <verb>bring soup to electric pot</verb>
    <gerund>bringing soup to electric pot</gerund>
    <priorityInType>20</priorityInType>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
  </WorkGiverDef>
  
   <WorkGiverDef>
    <defName>VCE_RemoveSoup</defName>
    <label>getting cooked soup from electric pot</label>
    <giverClass>VanillaCookingExpanded.WorkGiver_RemoveSoup</giverClass>
    <workType>Hauling</workType>
    <verb>get cooked soup from electric pot</verb>
    <gerund>getting cooked soup from electric pot</gerund>
    <priorityInType>20</priorityInType>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
  </WorkGiverDef>
  
 <WorkGiverDef>
    <defName>VCE_DoBillsGrill</defName>
    <label>cook at grill</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Cooking</workType>
    <priorityInType>100</priorityInType>
    <fixedBillGiverDefs>
      <li>VCE_Grill</li>   
    </fixedBillGiverDefs>
    <verb>grill</verb>
    <gerund>grilling at</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>

  
 
	
	



</Defs>