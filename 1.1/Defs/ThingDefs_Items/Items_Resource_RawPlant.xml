<?xml version="1.0" encoding="utf-8" ?>
<Defs>  


<ThingDef ParentName="RoughPlantBase">
    <defName>VCE_RawSugar</defName>
    <label>sugar</label>
    <description>Simple crystalline cane sugar, adds a much desired sweetness. While sugar is good on the tongue, it is not so good on the kidneys.</description>
    <graphicData>
      <texPath>Things/Items/Condiments/VCE_Sugar</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.9</MarketValue>
      <Flammability>1.3</Flammability>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>120</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <thingCategories>
      <li>VCE_Condiments</li>
    </thingCategories>
    <ingestible>
      <preferability>NeverForNutrition</preferability>
    </ingestible>
  </ThingDef>
  
  <ThingDef ParentName="RoughPlantBase">
    <defName>VCE_RawSpices</defName>
    <label>spices</label>
    <description>A variety of aromatic and pungent spices, adds several sharp notes on the tongue and puts a bit of a pep in your step.</description>
    <graphicData>
      <texPath>Things/Items/Condiments/VCE_Spices</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.9</MarketValue>
      <Flammability>1.3</Flammability>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>60</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <thingCategories>
      <li>VCE_Condiments</li>
    </thingCategories>
    <ingestible>
      <preferability>NeverForNutrition</preferability>
    </ingestible>
  </ThingDef>
	
	
	






</Defs>