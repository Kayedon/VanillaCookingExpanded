<?xml version="1.0" encoding="utf-8" ?>

<Defs>


<ThingDef ParentName="PlantFoodRawBase">
		<defName>VCE_RawApple</defName>
		<label>apples</label>
		<description>Glossy, red apples. Apples have been used as snacking food, a dessert ingredient, and brewed into cider since early mankind.</description>
		<soundInteract>Grain_Drop</soundInteract>
		<soundDrop>Grain_Drop</soundDrop>	

		<graphicData>

			<texPath>Things/Items/Raw/VCE_Apples</texPath>

		</graphicData>
		<statBases>
			<MarketValue>1.2</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>15</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>

		<ingestible>
		 <preferability>RawTasty</preferability>
			<foodType>VegetableOrFruit</foodType>
		</ingestible>    
	</ThingDef>
	
	<ThingDef ParentName="PlantFoodRawBase">
		<defName>VCE_RawPeach</defName>
		<label>peach</label>
		<description>An edible juicy fruit called peach or nectarine. A ripe peach will have a fragrant aroma and be delicious eaten fresh out of hand.</description>
		<soundInteract>Grain_Drop</soundInteract>
		<soundDrop>Grain_Drop</soundDrop>	

		<graphicData>

			<texPath>Things/Items/Raw/VCE_Peaches</texPath>

		</graphicData>
		<statBases>
			<MarketValue>1.2</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>15</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>

		<ingestible>
		 <preferability>RawTasty</preferability>
			<foodType>VegetableOrFruit</foodType>
		</ingestible>    
	</ThingDef>

  </Defs>
