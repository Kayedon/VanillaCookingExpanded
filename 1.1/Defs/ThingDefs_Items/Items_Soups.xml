<?xml version="1.0" encoding="utf-8" ?>
<Defs>  


	<ThingDef ParentName="RoughPlantBase">
		<defName>VCE_UncookedSoupSimple</defName>
		<label>uncooked simple soup</label>
		<description>An uncooked simple soup. This needs to be cooked in an electric pot to be edible.</description>
		<graphicData>
			<texPath>Things/Items/Uncookedsoup/VCE_UncookedSoupSimple</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>1.9</MarketValue>
			<Flammability>1.3</Flammability>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>20</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<thingCategories>
			<li>VCE_UncookedSoups</li>
		</thingCategories>
		<ingestible>
			<preferability>NeverForNutrition</preferability>			
		</ingestible>
	</ThingDef>
	
